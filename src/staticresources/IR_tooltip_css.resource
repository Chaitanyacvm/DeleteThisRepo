#tooltip {
 position: absolute;
 z-index: 3000;
 border: 1px solid #111;
 background-color: #eee;
 padding: 5px;
 opacity: 0.85;
 font-size:9px;
}
#tooltip h3 {
font-size:9px;
margin-top:0px;
padding-top:0px;
margin-bottom:0px;
padding-bottom:0px;
}
 
#tooltip div { margin: 0; }
 
.helpHover {
 font-size:9px;
}
